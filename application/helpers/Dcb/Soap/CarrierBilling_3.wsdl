<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<wsdl:definitions name="CarrierBilling" xmlns:m="http://www.android.google.com/billing"
  xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:http="http://schemas.xmlsoap.org/wsdl/http/"
  targetNamespace="http://www.android.google.com/billing">
  <wsdl:types>
    <xsd:schema targetNamespace="http://www.android.google.com/billing">
      <xsd:complexType name="Auth">
        <xsd:sequence>
          <xsd:element name="Version" type="xsd:int" maxOccurs="1"
            minOccurs="1">
            <xsd:annotation><xsd:documentation><![CDATA[
                The version of the protocol being used. This should be 3.
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
          <xsd:element name="CorrelationId" type="m:genericString128"
            maxOccurs="1" minOccurs="1">
            <xsd:annotation><xsd:documentation><![CDATA[
              Correlation ID to combine all the events related to an
              order. This ID is unique to each order.
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
          <xsd:element name="PurchaseTime" type="m:timestamp"
            maxOccurs="1" minOccurs="1">
            <xsd:annotation><xsd:documentation><![CDATA[
              Timestamp of the purchase request.
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
          <xsd:element name="BillingAgreement" type="m:genericString64"
            maxOccurs="1" minOccurs="1">
            <xsd:annotation><xsd:documentation><![CDATA[
              Short string indicating the billing agreement of the
              carrier who will charge the customer.  This will always
              be the same string, unless the same SOAP server is
              serving multiple carrier entities.
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
          <xsd:element name="OperatorUserToken" type="m:genericString" maxOccurs="1"
            minOccurs="1">
            <xsd:annotation><xsd:documentation><![CDATA[
              The Operator User Token for the user.
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
          <xsd:element name="PaymentDescription" type="m:genericString"
            maxOccurs="1" minOccurs="1">
            <xsd:annotation><xsd:documentation><![CDATA[
              Description of the purchase.
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
          <xsd:element name="SubscriptionId" type="m:genericString"
            maxOccurs="1" minOccurs="0">
            <xsd:annotation><xsd:documentation><![CDATA[
              If present, indicates that this transaction is for a
              recurring subscription and the value contained here is
              the subscription id for this subscription.  The same id
              that was present in the first Auth request to initiate
              the subscription will be present in every subsequent
              Auth request due to rebills on the same subscription.
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
          <xsd:element name="SubscriptionRecurrenceFrequency"
            type="m:SubscriptionRecurrence" maxOccurs="1" minOccurs="0">
            <xsd:annotation><xsd:documentation><![CDATA[
              This field will only be set if SubscriptionId is
              provided. It indicates how often the subscription will
              re-bill. (Note: DAILY is meant only for testing, so subscriptions
              can be tested with a short turnaround time. Daily subscriptions
              will not be officially supported in production.)
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
          <xsd:element name="MerchantContact" type="m:genericString"
            maxOccurs="1" minOccurs="1">
            <xsd:annotation><xsd:documentation><![CDATA[
              Contact information for the merchant, it may be a URL,
              phone number or address.  This field is free format, and
              is intended to be presented on the subscriber bill to
              allow consumers to contact the merchant.
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
          <xsd:element name="Currency" type="m:currencyCode"
            maxOccurs="1" minOccurs="1">
            <xsd:annotation><xsd:documentation><![CDATA[
              ISO 4217 3-letter currency code.
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
          <xsd:element name="ItemPrice" type="m:micros" maxOccurs="1"
            minOccurs="1">
            <xsd:annotation><xsd:documentation><![CDATA[
              The base price of the item; set by the
              developer. Revenue sharing applies only to this amount.
              ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
          <xsd:element name="Tax" type="m:micros" maxOccurs="1"
            minOccurs="1">
            <xsd:annotation><xsd:documentation><![CDATA[
              Computed tax on the base item price.
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
          <xsd:element name="Total" type="m:micros" maxOccurs="1"
            minOccurs="1">
            <xsd:annotation><xsd:documentation><![CDATA[
              Sum of ItemPrice and Tax; this is the amount which
              should be charged to the subscriber's bill.
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
          <xsd:element name="UserLocale" type="m:localeCode"
            maxOccurs="1" minOccurs="0">
            <xsd:annotation><xsd:documentation><![CDATA[
              A two-letter ISO 639-1 Alpha 2 language code
              optionally followed by a hyphen and an ISO 3166-1 Alpha-2
              country code, e.g."pt" or "pt-BR".
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
          <xsd:element name="DcbTosVersion" type="m:genericString64" maxOccurs="1"
            minOccurs="0">
            <xsd:annotation><xsd:documentation><![CDATA[
              Latest version of the Carrier Billing TOS that was
              accepted by the user.
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
          <xsd:element name="Passphrase" type="m:genericString"
            maxOccurs="1" minOccurs="0">
            <xsd:annotation><xsd:documentation><![CDATA[
              The passphrase to validate for this user account.
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>

      <xsd:simpleType name="SubscriptionRecurrence">
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="DAILY" />
          <xsd:enumeration value="MONTHLY" />
          <xsd:enumeration value="YEARLY" />
        </xsd:restriction>
      </xsd:simpleType>

      <xsd:complexType name="AuthResponse">
        <xsd:sequence>
          <xsd:element name="Version" type="xsd:int" maxOccurs="1"
            minOccurs="1">
            <xsd:annotation><xsd:documentation><![CDATA[
              The version of the protocol being used. Set to 3.
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
          <xsd:element name="CorrelationId" type="m:genericString128" maxOccurs="1"
            minOccurs="1">
            <xsd:annotation><xsd:documentation><![CDATA[
              Original CorrelationId for this response.
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
          <xsd:element name="Result" type="m:AuthResponseResult"
            maxOccurs="1" minOccurs="1">
            <xsd:annotation><xsd:documentation><![CDATA[
              <table border="1">
              <tr><th>Result Code</th><th>Description</th><th>System Behavior</th></tr>
              <tr>
                <td>SUCCESS</td>
                <td>Indicates that the operation succeeded normally.
                User receives confirmation email and app download starts.</td>
              </tr><tr>
                <td>CHARGE_EXCEEDS_LIMIT</td>
                <td>The total charge requested exceeds the
                  operator-determined bill cycle limit.  Returned for postpaid accounts.
                  Indicates that the subscriber needs to wait until the next bill cycle to
                  purchase or request limit increase from carrier.</td>
                <td>Payment marked as "declined"; no retry.</td>
              </tr><tr>
                <td>INSUFFICIENT_FUNDS</td>
                <td>Subscriber has insufficient funds for this
                  transaction.  Returned for prepaid accounts.  Indicates that the subscriber
                  needs to add funds to their account before proceeding with any more
                  transactions.</td>
                <td>Payment marked as "declined"; no retry.</td>
              </tr><tr>
                <td>INVALID_BILLING_AGREEMENT</td>
                <td>The operator has no record of the BillingAgreement, it does
                    not apply to the subscriber, or it has expired.</td>
                <td>Payment marked as "error"; no retry.</td>
              </tr><tr>
                <td>INVALID_CURRENCY</td>
                <td>Requested Currency cannot be handled by the operator</td>
                <td>Payment marked as "error"; no retry.</td>
              </tr><tr>
                <td>RETRIABLE_ERROR</td>
                <td>If any of the internal system is not accessible or throws a
                    retriable error, then Carriers should pass this as part
                    of the response code.</td>
                <td>The API call will be reattempted after some time.</td>
              </tr><tr>
                <td>GENERAL_DECLINE</td>
                <td>The authorization was declined for some other reason.
                  This code should be used only as last resort when all
                  other error codes do not apply.</td>
                <td>Payment marked as "declined"; no retry.</td>
              </tr><tr>
                <td>ACCOUNT_ON_HOLD</td>
                <td>Subscriber account is on hold due to negligent payments.
                  Indicates that the subscriber needs to pay their bill to have their account
                  reactivated for carrier billing.</td>
                <td>Payment marked as "declined"; no retry.</td>
              </tr><tr>
                <td>SUBSCRIPTION_CANCELLED</td>
                <td>The subscription id supplied was for a subscription
                  which has been cancelled by the user or the operator.</td>
                <td>Payment marked as "decline"; no retry.</td>
              </tr><tr>
                <td>NO_LONGER_PROVISIONED</td>
                <td>The device is no longer provisioned.  The carrier billing
                  option should no longer be presented to the user
                  until the next scheduled call to GetProvisioning,
                  normally 14 days.  Most often used to indicate that
                  the user has opted out of operator billing.
                </td>
                <td>Payment marked as "decline"; no retry.</td>
              </tr><tr>
                <td>INVALID_USER</td>
                <td>The user information provided in the call does not
                    correspond to an active user.  If the user was
                    previously associated and this error code is
                    encountered, the user will be disassociated.  This
                    is useful for users who have cancelled their
                    operator accounts.
                </td>
                <td>Payment marked as "decline"; no retry.</td>
              </tr><tr>
                <td>INVALID_TOS</td>
                <td>The TOS version supplied in the request isn't the
                    latest version of the TOS.  The user will have to
                    agree to the newest version of the TOS.  The new
                    TOS information MUST be provided in this response
                    by populating the DcbTos field.
                </td>
                <td>Payment marked as "decline"; no retry.</td>
              </tr><tr>
                <td>INVALID_PASSPHRASE_FAILURE</td>
                <td>Indicates that the supplied passphrase doesn't
                match the stored passphrase for this user.</td>
                <td>Payment marked as "decline"; no retry.</td>
              </tr></table>
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
          <xsd:element name="Message" type="m:genericString"
            maxOccurs="1" minOccurs="0">
            <xsd:annotation><xsd:documentation><![CDATA[
              A message explaning the result of the request (intended
              for debugging and logging).
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
          <xsd:element name="AuthTransactionId" type="m:genericString"
            maxOccurs="1" minOccurs="0">
            <xsd:annotation><xsd:documentation><![CDATA[
              A transaction id to be generated by the operator to
              uniquely identify this response.
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
          <xsd:element name="UserMessage" type="m:genericString"
            maxOccurs="1" minOccurs="0">
            <xsd:annotation><xsd:documentation><![CDATA[
              An error message to be shown to the end user. This should be
              translated into the user's language as specified in the
              UserLocale field of the request. If this message is missing
              the user will be shown a generic error message based
              on the Result code.
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
          <xsd:element name="DcbTos" type="m:CarrierTos" maxOccurs="1"
            minOccurs="0">
            <xsd:annotation><xsd:documentation><![CDATA[
              If present, the user must be presented with and agree to
              this Terms of Service prior to any operator billing
              purchases.
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>

      <xsd:complexType name="CancelNotification">
        <xsd:sequence>
          <xsd:element name="Version" type="xsd:int" maxOccurs="1"
            minOccurs="1">
            <xsd:annotation><xsd:documentation><![CDATA[
              The version of the protocol being used. This should be 3.
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
          <xsd:element name="CorrelationId" type="m:genericString128"
            maxOccurs="1" minOccurs="1">
            <xsd:annotation><xsd:documentation><![CDATA[
              The CorrelationId of the Auth request to be cancelled.
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
          <xsd:element name="BillingAgreement" type="m:genericString64"
            maxOccurs="1" minOccurs="1">
            <xsd:annotation><xsd:documentation><![CDATA[
              Short string indicating the billing agreement of the
              carrier who will charge the customer.  This will always
              be the same string, unless the same SOAP server is
              serving multiple carrier entities.
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>

      <xsd:simpleType name="AuthResponseResult">
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="SUCCESS" />
          <xsd:enumeration value="CHARGE_EXCEEDS_LIMIT" />
          <xsd:enumeration value="INSUFFICIENT_FUNDS" />
          <xsd:enumeration value="INVALID_BILLING_AGREEMENT" />
          <xsd:enumeration value="INVALID_CURRENCY" />
          <xsd:enumeration value="RETRIABLE_ERROR" />
          <xsd:enumeration value="GENERAL_DECLINE" />
          <xsd:enumeration value="ACCOUNT_ON_HOLD" />
          <xsd:enumeration value="SUBSCRIPTION_CANCELLED" />
          <xsd:enumeration value="NO_LONGER_PROVISIONED" />
          <xsd:enumeration value="INVALID_USER" />
          <xsd:enumeration value="INVALID_TOS" />
          <xsd:enumeration value="INVALID_PASSPHRASE_FAILURE" />
        </xsd:restriction>
      </xsd:simpleType>

      <xsd:complexType name="Echo">
        <xsd:sequence>
          <xsd:element name="Version" type="xsd:int" maxOccurs="1"
            minOccurs="1">
            <xsd:annotation><xsd:documentation><![CDATA[
              The version of the protocol being used. Should be 3.
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
          <xsd:element name="CorrelationId" type="m:genericString128"
            maxOccurs="1" minOccurs="1">
            <xsd:annotation><xsd:documentation><![CDATA[
              The unique id for this request.
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
          <xsd:element name="Message" type="m:genericString"
            maxOccurs="1" minOccurs="1">
            <xsd:annotation><xsd:documentation><![CDATA[
              Google will send a test message and this should be
              returned back in the response.
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
          <xsd:element name="BillingAgreement" type="m:genericString64"
            maxOccurs="1" minOccurs="1">
            <xsd:annotation><xsd:documentation><![CDATA[
              Short string indicating the billing agreement of the
              carrier who will charge the customer.  This will always
              be the same string, unless the same SOAP server is
              serving multiple carrier entities.
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>

      <xsd:complexType name="EchoResponse">
        <xsd:sequence>
          <xsd:element name="Version" type="xsd:int" maxOccurs="1"
            minOccurs="1">
            <xsd:annotation><xsd:documentation><![CDATA[
              The version of the protocol being used. Set to 3.
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
          <xsd:element name="CorrelationId" type="m:genericString128"
            maxOccurs="1" minOccurs="1">
            <xsd:annotation><xsd:documentation><![CDATA[
              Original CorrelationId for this response.
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
          <xsd:element name="EchoTransactionId" type="m:genericString" maxOccurs="1"
            minOccurs="0">
            <xsd:annotation><xsd:documentation><![CDATA[
              A transaction id to be generated by the operator to
              uniquely identify this response.
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
          <xsd:element name="Result" type="m:EchoResponseResult"
            maxOccurs="1" minOccurs="1">
            <xsd:annotation><xsd:documentation><![CDATA[
              <table border="1">
              <tr><th>Result Code</th><th>Description</th></tr>
              <tr>
                <td>SUCCESS</td>
                <td>Indicates that the operation succeeded normally.</td>
              </tr><tr>
                <td>GENERAL_FAILURE</td>
                <td>Indicates that there is a fatal error with the system.</td>
              </tr><tr>
                <td>RETRIABLE_ERROR</td>
                <td>If any of the internal system is not accessible or throws a
                    retriable error, then Carriers should pass this as part
                    of the response code.</td>
              </tr></table>
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
          <xsd:element name="OriginalMessage" type="m:genericString"
            maxOccurs="1" minOccurs="1">
            <xsd:annotation><xsd:documentation><![CDATA[
              Carriers should tunnel back the original message that
              was sent as part of the Echo Request.
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
          <xsd:element name="DebugMessage" type="m:genericString"
            maxOccurs="1" minOccurs="0">
            <xsd:annotation><xsd:documentation><![CDATA[
              A debug string that will be useful for debugging.
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>

      <xsd:simpleType name="EchoResponseResult">
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="SUCCESS" />
          <xsd:enumeration value="GENERAL_FAILURE" />
          <xsd:enumeration value="RETRIABLE_ERROR" />
        </xsd:restriction>
      </xsd:simpleType>

      <xsd:simpleType name="genericString">
        <xsd:restriction base="xsd:string">
          <xsd:minLength value="0" />
          <xsd:maxLength value="1024" />
        </xsd:restriction>
      </xsd:simpleType>

      <xsd:simpleType name="genericString64">
        <xsd:restriction base="xsd:string">
          <xsd:minLength value="0" />
          <xsd:maxLength value="64" />
        </xsd:restriction>
      </xsd:simpleType>

      <xsd:simpleType name="genericString128">
        <xsd:restriction base="xsd:string">
          <xsd:minLength value="0" />
          <xsd:maxLength value="128" />
        </xsd:restriction>
      </xsd:simpleType>

      <xsd:simpleType name="genericString256">
        <xsd:restriction base="xsd:string">
          <xsd:minLength value="0" />
          <xsd:maxLength value="256" />
        </xsd:restriction>
      </xsd:simpleType>

      <xsd:simpleType name="timestamp">
        <xsd:restriction base="xsd:long" />
      </xsd:simpleType>

      <xsd:simpleType name="currencyCode">
        <xsd:restriction base="xsd:string">
          <xsd:whiteSpace value="collapse" />
          <xsd:pattern value="[A-Z]{3}" />
        </xsd:restriction>
      </xsd:simpleType>

      <xsd:simpleType name="countryCode">
        <xsd:restriction base="xsd:string">
          <xsd:whiteSpace value="collapse" />
          <xsd:pattern value="[A-Z]{2}" />
        </xsd:restriction>
      </xsd:simpleType>

      <xsd:simpleType name="localeCode">
        <xsd:restriction base="xsd:string">
          <xsd:whiteSpace value="collapse" />
          <xsd:pattern value="[a-z]{2}(-[A-Z]{2})?" />
        </xsd:restriction>
      </xsd:simpleType>

      <xsd:simpleType name="micros">
        <xsd:restriction base="xsd:long" />
      </xsd:simpleType>

      <xsd:complexType name="GetProvisioning">
        <xsd:sequence>
          <xsd:element name="Version" type="xsd:int" maxOccurs="1"
            minOccurs="1">
            <xsd:annotation><xsd:documentation><![CDATA[
              The version of the protocol being used. Set to 3.
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
          <xsd:element name="CorrelationId" type="m:genericString128"
            maxOccurs="1" minOccurs="1">
            <xsd:annotation><xsd:documentation><![CDATA[
              The unique id for this request.
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
          <xsd:element name="UserIdentifier" type="m:UserIdentifier" maxOccurs="1"
            minOccurs="1">
            <xsd:annotation><xsd:documentation><![CDATA[
              The fields contained in here identifier the user.
              SubscriberId, DeviceId and SimSerialNumber are not
              set and will be removed in future.
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
          <xsd:element name="UserLocale" type="m:localeCode"
            maxOccurs="1" minOccurs="0">
            <xsd:annotation><xsd:documentation><![CDATA[
              A two-letter ISO 639-1 Alpha 2 language code
              optionally followed by a hyphen and an ISO 3166-1 Alpha-2
              country code, e.g."pt" or "pt-BR".
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
          <xsd:element name="BillingAgreement" type="m:genericString64"
            maxOccurs="1" minOccurs="1">
            <xsd:annotation><xsd:documentation><![CDATA[
              Short string indicating the billing agreement of the
              carrier who will charge the customer.  This will always
              be the same string, unless the same SOAP server is
              serving multiple carrier entities.
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>

      <xsd:complexType name="GetProvisioningResponse">
        <xsd:sequence>
          <xsd:element name="Version" type="xsd:int" maxOccurs="1"
             minOccurs="1">
            <xsd:annotation><xsd:documentation><![CDATA[
              The version of the protocol being used. Set to 3.
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
           <xsd:element name="CorrelationId" type="m:genericString128"
            maxOccurs="1" minOccurs="1">
            <xsd:annotation><xsd:documentation><![CDATA[
              Original CorrelationId for this response.
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
          <xsd:element name="Result" type="m:GetProvisioningResponseResult"
            maxOccurs="1" minOccurs="1">
            <xsd:annotation><xsd:documentation><![CDATA[
              <table border="1">
              <tr><th>Result Code</th><th>Description</th></tr>
              <tr>
                <td>SUCCESS</td>
                <td>Indicates that the operation succeeded normally.
                User receives confirmation email and app download starts.</td>
              </tr><tr>
                <td>INVALID_BILLING_AGREEMENT</td>
                <td>The operator has no record of the BillingAgreement, it does
                    not apply to the subscriber, or it has expired.</td>
              </tr><tr>
                <td>INVALID_USER</td>
                <td>The user information provided in the call does not
                    correspond to an active user.  If the user was
                    previously associated and this error code is
                    encountered, the user will be disassociated.  This
                    is useful for users who have cancelled their
                    operator accounts.
                </td>
              </tr><tr>
                <td>GENERAL_FAILURE</td>
                <td>Indicates that an error has happened, but retrying the
                    request at a later time has a chance of getting an answer.
                    This is not the appropriate return code for a user who
                    cannot be provisioned or is not provisioned. If the
                    UserIdentifier is not recognized, return INVALID_USER. If
                    the UserIdentifier is recognized but the user is not
                    provisioned, return SUCCESS and IsProvisioned = false.</td>
              </tr><tr>
              </tr></table>
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
          <xsd:element name="Message" type="m:genericString"
            maxOccurs="1" minOccurs="0">
            <xsd:annotation><xsd:documentation><![CDATA[
              A message explaning the result of the request (intended
              for debugging and logging).
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
          <xsd:element name="IsProvisioned" type="xsd:boolean" maxOccurs="1"
            minOccurs="0">
            <xsd:annotation><xsd:documentation><![CDATA[
              Indicates whether or not operator billing is allowed for
              this subscriber. Only required if the
              Result is SUCCESS.
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
          <xsd:element name="PiiTos" type="m:CarrierTos" maxOccurs="1"
            minOccurs="0">
            <xsd:annotation><xsd:documentation><![CDATA[
              If present, the user must be presented with and agree to
              this Terms of Service prior to the operator providing
              any Subscriber Information to Google about the user.  If
              a combined TOS should be used, PiiTos and DcbTos should
              be set to the same values.
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
          <xsd:element name="DcbTos" type="m:CarrierTos" maxOccurs="1"
            minOccurs="0">
            <xsd:annotation><xsd:documentation><![CDATA[
              If present, the user must be presented with and agree to
              this Terms of Service prior to any operator billing
              purchases.  If a combined TOS should be used, PiiTos and
              DcbTos should be set to the same values.
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
          <xsd:element name="SubscriberCurrency" type="m:currencyCode" maxOccurs="1"
            minOccurs="0">
            <xsd:annotation><xsd:documentation><![CDATA[
              ISO 4217 3-letter code of the accepted currency for this
              account. Only required if the Result is SUCCESS and IsProvisioned is true.
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
          <xsd:element name="TransactionLimit" type="m:micros" maxOccurs="1"
            minOccurs="0">
            <xsd:annotation><xsd:documentation><![CDATA[
              The maximum limit of a single transaction allowed by the
              operator for this subscriber.  Must be an integer
              greater than 0. Only required if the Result is SUCCESS and IsProvisioned is true.
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
          <xsd:element name="AccountType" type="m:AccountType" maxOccurs="1"
            minOccurs="0">
            <xsd:annotation><xsd:documentation><![CDATA[
              <table border="1">
                <tr>
                  <th>Account Type</th>
                  <th>Description</th>
                </tr>
                <tr>
                  <td>GENERAL</td>
                  <td>A general account with no special limitations.</td>
                </tr>
                <tr>
                  <td>PREPAID</td>
                  <td>Special account that is prepaid by the user.</td>
                </tr>
                <tr>
                  <td>ENTERPRISE</td>
                  <td>An enterprise managed account, where the enterprise may
                    have placed limitations on purchases.</td>
                </tr>
              </table>
              Only required if the Result is SUCCESS and IsProvisioned is true.
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
          <xsd:element name="GetProvisioningTransactionId" type="m:genericString"
            maxOccurs="1" minOccurs="0">
            <xsd:annotation><xsd:documentation><![CDATA[
              A transaction id to be generated by the operator to
              uniquely identify this response.
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
          <xsd:element name="UserMessage" type="m:genericString"
            maxOccurs="1" minOccurs="0">
            <xsd:annotation><xsd:documentation><![CDATA[
              An error message to be shown to the end user. This should be
              provided in the user's language as specified in the
              UserLocale field of the request. If this message is missing
              the user will be shown a generic error message based
              on the Result code.
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
          <xsd:element name="PasswordPrompt" type="m:genericString"
            maxOccurs="1" minOccurs="0">
            <xsd:annotation><xsd:documentation><![CDATA[
              A localized message to be shown to the user to be used
              when asking them to enter their passphrase.
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
          <xsd:element name="ForgottenPasswordUrl" type="m:genericString"
            maxOccurs="1" minOccurs="0">
            <xsd:annotation><xsd:documentation><![CDATA[
              The URL to send a user to if then forget their password
              and need to recover it.
            ]]></xsd:documentation></xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:complexType>

      <xsd:simpleType name="GetProvisioningResponseResult">
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="SUCCESS" />
          <xsd:enumeration value="INVALID_BILLING_AGREEMENT" />
