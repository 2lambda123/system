<CallGeneratorReport>
	<From><?= date(Billrun_Base::base_dateformat,$data['from']) ?></From>
	<To><?= date(Billrun_Base::base_dateformat, $data['to']) ?></To>
	<Summary>
		<Missed><?= $data['summary']['missed'] ?></Missed>
		<DurationDifference><?= $data['summary']['duration_diff'] ?></DurationDifference>
		<CallTypes>
			<?php foreach ($data['summary']['calls_types'] as $key => $value) {	?>
				<<?= ucfirst($key) ?>><?= $value ?></<?= ucfirst($key) ?>>
			<?php } ?>
		</CallTypes>
	<Summary>
	<Comparison>
		<?php foreach ($data['cdr_calls_comparison'] as $key => $compr) {	?>
