			<Generated>
				<?php if (isset($compr['generated'])) {					
						 foreach ($compr['generated'] as $key => $value) {	?>
						<<?= ucfirst($key) ?>><?= $value ?></<?= ucfirst($key) ?>>
					<?php } 
					} ?>								
			</Generated>
			<CDR>
				<?php if (isset($compr['cdr'])) {
						 foreach ($compr['cdr'] as $key => $value) {	?>
						<<?= ucfirst($key) ?>><?= $value ?></<?= ucfirst($key) ?>>
					<?php } 
					 } ?>
			</CDR>
		</CallComparison>
		<?php } ?>
	</Comparison>
	<CallsBreakdown>
		<?php foreach ($data['generated_breakdown'] as $key => $value) { ?>
			<<?= ucfirst($key) ?>><?= count($value )?></<?= ucfirst($key) ?>>
		<?php } ?>
	</CallsBreakdown>
